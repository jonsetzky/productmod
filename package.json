{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "description": "Boilerplate for vanilla node projects",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --watch src/schemas build.js",
    "build": "node build.js",
    "start": "node src/",
    "test": "jest --verbose --runInBand",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage src",
    "fixtures": "run-func test/fixtures/generate.js generate"
  },
  "keywords": [],
  "dependencies": {
    "axios": "^1.6.0",
    "axios-mock-adapter": "^1.21.5",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.6.2",
    "joi": "^17.9.2",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.1",
    "yesno": "^0.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "joi-to-typescript": "^4.6.0",
    "nodemon": "^3.0.1",
    "run-func": "^3.0.0"
  },
  "nodemonConfig": {
    "delay": 500
  }
}
